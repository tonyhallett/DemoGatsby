{
    "launch": {
        "version": "0.2.0",
        "configurations": [
          {
            "name": "Gatsby develop",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
            "args": ["develop"],
            "runtimeArgs": ["--nolazy"],
            "console": "integratedTerminal"
          },
          {
            "name": "Gatsby build",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
            "args": ["build","--verbose"],
            "runtimeArgs": ["--nolazy"],
            "console": "integratedTerminal"
          },
          {
            "name": "Gatsby build global",
            "type": "pwa-node",
            "request": "launch",
            "program": "${env:NVM_SYMLINK}/node_modules/gatsby-cli/cli.js",
            "args": ["build","--verbose"],
            "runtimeArgs": ["--nolazy"],
            "console": "integratedTerminal"
          }
        ]
      }
}